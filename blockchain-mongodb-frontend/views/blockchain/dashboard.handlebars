                        <li class="in">
                            <form role="search" id="search" action="blockByHeight" method="post" class="app-search d-none d-md-block mr-3">
                                <input type="text" name="search" placeholder="Search..." class="form-control mt-0">
                                 {{!-- <label>Select</label> --}}
                                    <select id="selectBox" class="searchSelect" onclick="searchBarr()">
                                    <option disabled>Select</option> 
                                    <option value="blockByHeight">block By height</option> 
                                    <option value="blockByHash">block By hash</option> 
                                    <option value="address">Transaction by address</option>
                                    <option value="tractionByHeight">Transaction by Block Height</option> 
 
                                    </select>
                                <button type="submit" class="active search-submit-btn">
                                    <i class="fa fa-search"></i>
                                </button>
                            </form>
                        </li>
                        <script>
                            function searchBarr(){
                                var Action = document.getElementById("search");
                                var e = document.getElementById("selectBox");
                                var searchItem = e.value;
                                Action.action=searchItem
                                console.log(searchItem)
                            }
