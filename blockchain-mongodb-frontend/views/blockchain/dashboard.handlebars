        <div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <div class="page-breadcrumb bg-white" style="padding: 20px 25px;">
                <div class="row align-items-center">
                    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                        <h4 class="page-title text-uppercase font-medium font-14">Dashboard</h4>
                    </div>
                   
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- ============================================================== -->
            <!-- End Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Container fluid  -->
            <!-- ============================================================== -->
            <div class="container-fluid">
                <!-- ============================================================== -->
                <!-- Three charts -->
                <!-- ============================================================== -->
                <div class="row " style="justify-content: space-between;">
                    <div class="col-lg-4 col-sm-6 col-xs-12">
                        <div class="white-box analytics-info">
                            <h3 class="box-title">  Block Size</h3>
                            <ul class="list-inline two-part d-flex align-items-center mb-0">
                                <li>
                                    <div id="sparklinedash"><canvas width="67" height="30"
                                            style="display: inline-block; width: 67px; height: 30px; vertical-align: top;"></canvas>
                                    </div>
                                </li>
                                <li class="ml-auto"><span class="counter text-success">{{blocksize.length}}</span></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-4 col-sm-6 col-xs-12">
                        <div class="white-box analytics-info">
                            <h3 class="box-title">Mempool Transaction</h3>
                            <ul class="list-inline two-part d-flex align-items-center mb-0">
                                <li>
                                    <div id="sparklinedash2"><canvas width="67" height="30"
                                            style="display: inline-block; width: 67px; height: 30px; vertical-align: top;"></canvas>
                                    </div>
                                </li>
                                <li class="ml-auto"><span class="counter text-purple">{{mempool.length}}</span></li>
                            </ul>
                        </div>
                    </div>
                    
                </div>
                <!-- ============================================================== -->
               
                <!-- ============================================================== -->
                <!-- Blocks -->
                <!-- ============================================================== -->
                <div class="row">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <div class="white-box">
                            <div class="d-md-flex mb-3">
                                <h3 class="box-title mb-0">Blocks</h3>
                               
                            </div>
                            <div class="table-responsive">
                                <table class="table no-wrap">
                                    <thead>
                                        <tr>
                                            <th class="border-top-0">Height</th>
                                            <th class="border-top-0">Timestamp</th>
                                            <th class="border-top-0">Transactions</th>
                                            <th class="border-top-0">PreviousHash</th>
                                            <th class="border-top-0">Hash</th>
                                            <th class="border-top-0">Nonce</th>
                                        </tr>
                                    </thead>
                                    <tbody> 
                                          {{#each block}}
                                        <tr>
                                            <td>{{this.height}}</td>
                                            <td class="txt-oflo">{{this.timestamp}}</td>
                                            <td style="width: 50px;">{{this.transactions.length}}</td>
                                            <td class="txt-oflo">{{this.previousHash}}</td>
                                            <td><span class="txt-oflo">{{this.hash}}</span></td>
                                            <td><span class="text-success">{{this.nonce}}</span></td>
                                        </tr>
                                           {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- ============================================================== -->
                <!-- Mempool -->
                <!-- ============================================================== -->
                <div class="row">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <div class="white-box">
                            <div class="d-md-flex mb-3">
                                <h3 class="box-title mb-0">Mempool</h3>
                            </div>
                            <div class="table-responsive">
                                <table class="table no-wrap">
                                    <thead>
                                        <tr>
                                            <th class="border-top-0">Timestamp</th>
                                            <th class="border-top-0">Amount</th>
                                            <th class="border-top-0">Sender</th>
                                            <th class="border-top-0">Receiver</th>
                                            <th class="border-top-0">Transaction Hash</th>
                                        </tr>
                                    </thead>
                                    <tbody> 
                                        {{#each mempool}}
                                        <tr>
                                            <td>{{this.timestamp}}</td>
                                            <td class="txt-oflo">{{this.amount}}</td>
                                            <td class="txt-oflo">{{this.sender}}</td>
                                            <td style="width: 50px;">{{this.receiver}}</td>
                                            <td><span class="txt-oflo">{{this.txHash}}</span></td>
                                        </tr>
                                           {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
             
            </div>
